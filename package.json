{
	"name": "augmented-debug",
	"displayName": "Augmented Debug",
	"description": "Augment any debugger",
	"version": "0.0.1",
	"license": "MIT",
	"engines": {
		"vscode": "^1.42.0"
	},
	"activationEvents": [
		"onDebugAdapterProtocolTracker"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"dependencies": {
		"vscode-debugprotocol": "1.37.0",
		"vscode-debugadapter": "1.38.0-pre.4"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/node": "^12.11.7",
		"vscode": "1.1.36",
		"glob": "^7.1.5",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}
